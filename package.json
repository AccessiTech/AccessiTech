{
  "name": "@accessitech/homepage",
  "private": false,
  "version": "2.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "yarn dev",
    "rss": "node ./scripts/rss.js",
    "sitemap": "node ./scripts/sitemap.js",
    "pre:build": "yarn rss && yarn sitemap && tsc",
    "build": "yarn pre:build && vite build && yarn post:build",
    "post:build": "cp docs/index.html docs/404.html && cp CNAME docs/CNAME",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-social-icons": "^6.22.0",
    "reduxjs-toolkit-persist": "^7.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "bootstrap": "^5.3.1",
    "eslint": "^8.48.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "react-bootstrap": "^2.8.0",
    "sass": "^1.66.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
